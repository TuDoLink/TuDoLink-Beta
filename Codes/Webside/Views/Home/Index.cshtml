    <script>
window.onscroll = function () {
var topbox = window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop;
var navbox = document.getElementById('navbar');
if (topbox >800) {
    navbox.style.background = '#2a333b';
} else {
